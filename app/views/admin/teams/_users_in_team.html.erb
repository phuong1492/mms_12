<div class="col-xs-6">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th><%= t "admin.team.all_user" %></th>
      </tr>
    </thead>
    <tbody>
      <%= f.collection_check_boxes :user_ids, @team.users, :id, :name do |b| %>
        <tr>
          <td><%= b.check_box %><%= b.object.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
