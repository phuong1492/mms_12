<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @skill] do |f| %>
      <% render "shared/error_messages" %>

      <%= f.label :skill_name %>
      <%= f.text_field :skill_name, class: "form-control"%>
      
      <%= f.submit t("skill.createskill"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>